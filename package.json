{
  "name": "tezos-lottery",
  "version": "1.0.0",
  "description": "Lottery dapp on Tezos",
  "scripts": {
    "sync": "node ./utils/scripts/compileSync.js",
    "test": "bash ./utils/scripts/checkScenario.sh",
    "compile": "npm run run-code",

  "license": "MIT",
  "dependencies": {
    "@taquito/taquito": "^6.3.0-wallet.3",
    "bs58check": "^2.1.2",
    "chalk": "^4.1.0",
    "conseiljs": "0.4.3",
    "libsodium-wrappers-sumo": "^0.7.6"
  }
}
